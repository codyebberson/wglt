    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^28.1.3",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "babel-loader": "^8.2.5",
    "eslint": "^8.18.0",
    "jest": "^28.1.1",
    "terser-webpack-plugin": "^5.3.3",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
